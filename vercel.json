{
    "version": 2,
    "builds": [
      {
        "src": "django_project/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/frontend/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "django_project/index.py"
      }
    ]
  }
  